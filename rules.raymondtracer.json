{
	"$schema": "https://raw.githubusercontent.com/PazerOP/tf2_bot_detector/master/schemas/v3/rules.schema.json",
	"file_info": {
		"authors": [
			"Raymond Tracer"
		],
		"title": "Raymond Tracer's Rule List",
		"update_url": "https://raw.githubusercontent.com/RaymondTracer/RT-TF2BD-Rules/main/rules.raymondtracer.json"
	},
	"rules": [
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Regex Name match",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"(\\(\\d+\\))?\\d{17}",
						"(\\(\\d+\\))?https://octogaming.tk/mcd",
						"(\\(\\d+\\))?quizno",
						"(\\(\\d+\\))?Lucien",
						"(\\(\\d+\\))?\\(CORPORATE\\) Cvnt",
						"(\\(\\d+\\))?Ninjabot #\\d{4}"
					]
				}
			}
		}
	]
}